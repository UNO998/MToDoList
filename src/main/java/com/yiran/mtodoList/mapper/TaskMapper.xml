        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yiran.mtodoList.mapper.TaskMapper">

	<insert id="addTask">
		INSERT INTO task SET 
		title=#{title},description=#{description},dueDate=#{dueDate},state=#{state}
	</insert>

</mapper>